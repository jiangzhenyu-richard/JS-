<script>
    //1.字符串切割
    // 将数组转换成字符串
    let arr = [1,3,5];
    console.log(arr);
    let str = arr.join("-");
    console.log(str);
    //将字符串转换成数组
    let str1 = "1,3,6";
    console.log(str1);
    let arr1 = str1.split("-");
    console.log(arr1);

    //2.判断是否以制定字符串开头
    let str2 = "http://www.sadc.com";
    let str3 = str2.startsWith("www");
    console.log(str3);

    //3.判断是否以制定字符串结尾
    let str4 = "http://www.sadc.com";
    let str5 = str4.endsWith("sadc.com");
    console.log(str5);

    //4.
    let name = "lnj";
    let age = 34;
    let arrr1 = `我是的名字是${name}我是年龄是${age}`;
    console.log(arrr1);
</script>
</html>
